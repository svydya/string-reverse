    "#to reverse a string\n",
    "def reverse(s):\n",
    "    str = \"\"\n",
    "    for i in s:\n",
    "        str = i + str\n",
    "    return str\n",
    "\n",
    "print(\"enter the input word\")\n",
    "word = input()\n",
    "\n",
    "print(\"the given string is:\", end =\"\")\n",
    "print(word)\n",
    "\n",
    "print(\"the reverse string is:\",end =\"\")\n",
    "print(reverse(word))"
   ]
